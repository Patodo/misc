
Echo off 

echo  自动获取IP地址....
echo  自动获取IP地址....
netsh interface ip set address name = "本地连接" source = dhcp
echo  自动获取DNS服务器....
netsh interface ip set dns name = "本地连接" source = dhcp
netsh interface ip set address name = "本地连接 2" source = dhcp
echo  自动获取DNS服务器....
netsh interface ip set dns name = "本地连接 2" source = dhcp
echo  自动获取IP地址....
netsh interface ip set address name = "本地连接 3" source = dhcp
echo  自动获取DNS服务器....
netsh interface ip set dns name = "本地连接 3" source = dhcp
echo  自动获取IP地址....


Pause

